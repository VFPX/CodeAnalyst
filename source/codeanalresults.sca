THIS.cmdPrint.Picture = "bitmaps\print.bmp"
THIS.cmdPrint.PicturePosition = 1

THIS.cmdExport.AutoSize = .T.
THIS.cmdExport.Picture = "bitmaps\export.bmp"
THIS.cmdExport.PicturePosition = 1

